<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.internetrequest">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.InternetRequest">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".a_one.OneActivityForKotlin" android:exported="true"/>
        <activity android:name=".a_one.OneActivityForKotlinRefactor" android:exported="true"/>

        <activity android:name=".b_two.TwoActivity" android:exported="true"/>
        <activity android:name=".b_two.TwoActivityRefactor" android:exported="true"/>

        <activity android:name=".c_three.ThreeActivity" android:exported="true"/>
        <activity android:name=".d_four.FourActivity" android:exported="true"/>
        <activity android:name=".e_five.FiveActivity" android:exported="true"/>
    </application>

</manifest>